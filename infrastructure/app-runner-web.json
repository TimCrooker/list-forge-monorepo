{
  "ServiceName": "listforge-web",
  "SourceConfiguration": {
    "ImageRepository": {
      "ImageIdentifier": "058264088602.dkr.ecr.us-east-1.amazonaws.com/listforge-web:latest",
      "ImageConfiguration": {
        "Port": "80",
        "RuntimeEnvironmentVariables": {}
      },
      "ImageRepositoryType": "ECR"
    },
    "AuthenticationConfiguration": {
      "AccessRoleArn": "arn:aws:iam::058264088602:role/AppRunnerECRAccessRole"
    },
    "AutoDeploymentsEnabled": true
  },
  "InstanceConfiguration": {
    "Cpu": "256",
    "Memory": "512"
  },
  "HealthCheckConfiguration": {
    "Protocol": "HTTP",
    "Path": "/",
    "Interval": 10,
    "Timeout": 5,
    "HealthyThreshold": 1,
    "UnhealthyThreshold": 5
  }
}
