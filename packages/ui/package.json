    "date-fns": "^2.30.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.16.0",
    "immer": "^10.1.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.292.0",
    "next-themes": "^0.4.6",
