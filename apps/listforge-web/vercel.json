{
  "headers": [
    {
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ],
      "source": "/assets/(.*)"
    }
  ],
  "rewrites": [
    {
      "destination": "https://api.list-forge.ai/api/$1",
      "source": "/api/(.*)"
    },
    {
      "destination": "/index.html",
      "source": "/(.*)"
    }
  ]
}